<h1>New analytical_offering</h1>

<% form_for(@analytical_offering) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :function %><br />
    <%= f.text_field :function %>
  </p>
  <p>
    Save results for this function in this folder structure:<br />
    <%= f.text_field :save_path %>
  </p>
  <p>
    <%= f.label :source_code_link %><br />
    <%= f.text_field :source_code_link %>
  </p>
  <p>
    <%= f.label :created_by %><br />
    <%= f.text_field :created_by %>
  </p>
  <p>
    <%= f.label :created_by_link %><br />
    <%= f.text_field :created_by_link %>
  </p>
  <p>
    <%= f.label :access_level %><br />
    <%= f.select :access_level, ["Select An Access Level", "User", "Private Researcher", "Commercial Account", "Admin"], :value => "Select An Access Level" %>    
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', analytical_offerings_path %>